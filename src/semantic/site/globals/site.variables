/*******************************
     User Global Variables
*******************************/

/* Fonts */
@headerFont: 'Poppins Black';
@subheaderFont: 'Poppins';
@pageFont: 'Roboto';
@fontName : 'Roboto';

@fontSize : 18px;
@importGoogleFonts : false;


/* Colors */
@bluec42: #6BA3D6;
@yellowc42: #FFE800;
@greyc42: #424242;

@blue: @bluec42;
@yellow: @yellowc42;
@grey: @greyc42;

@primary: @bluec42

/* Links */
@linkColor: @greyc42;
@linkHoverColor: @greyc42;
@linkHoverUnderline: none;
